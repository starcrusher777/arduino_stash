void handleNotFound(){
  digitalWrite(led, 1);
  String message = "File Not Found\n\n";
  message += "URI: ";
  message += server.uri();
  message += "\nMethod: ";
  message += (server.method() == HTTP_GET)?"GET":"POST";
  message += "\nArguments: ";
  message += server.args();
  message += "\n";
  for (uint8_t i=0; i<server.args(); i++){
    message += " " + server.argName(i) + ": " + server.arg(i) + "\n";
  }
  server.send(404, "text/plain", message);
  digitalWrite(led, 0);
}

void setup(void){

  Serial.begin(9600);
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);
  display.clearDisplay();
  delay(2000);


  pinMode(led, OUTPUT);
  digitalWrite(led, 0);
  Serial.begin(115200);
  WiFi.mode(WIFI_STA);
  WiFi.begin(ssid, password);
  Serial.println("");

  // Wait for connection
  while (WiFi.status() != WL_CONNECTED) {
    delay(500);
    Serial.print(".");
  }
  Serial.println("");
  Serial.print("Connected to ");
  Serial.println(ssid);
  Serial.print("IP address: ");
  Serial.println(WiFi.localIP());

  if (MDNS.begin("esp8266")) {
    Serial.println("MDNS responder started");
  }

  server.on("/", handleRoot);
  server.on("/inline", [](){
    server.send(200, "text/plain", "this works as well");
  });

  server.on("/left", HTTP_GET, [](AsyncWebServerRequest *request){
        left = 1;
        right = 0;
        display.clearDisplay();
        display.drawBitmap(0, 0, _pawsonair, SCREEN_WIDTH, SCREEN_HEIGHT, 1);
        display.display();
        request->send(200, "text/plain", "Left button pressed");
    });

  server.on("/right", HTTP_GET, [](AsyncWebServerRequest *request){
        left = 0;
        right = 1;
        display.clearDisplay();
        display.drawBitmap(0, 0, _rightpawontable, SCREEN_WIDTH, SCREEN_HEIGHT, 1);
        display.display();
        request->send(200, "text/plain", "Right button pressed");
  });

  server.onNotFound(handleNotFound);

  server.begin();
  Serial.println("HTTP server started");
}

void loop(void){
  server.handleClient();
}
